# MinIO test environment
# Source this file to test S3 downloads with local MinIO server
#
# Usage:
#   start local minio server
#   source testdata/minio-test.env
#   ./sdflow example-s3-loader-to-disk

# AWS-style environment variables (for built-in SDK)
export AWS_ACCESS_KEY_ID=myaccesskey
export AWS_SECRET_ACCESS_KEY=mysecretkey
export AWS_ENDPOINT_URL=http://127.0.0.1:9000
export AWS_S3_FORCE_PATH_STYLE=true
export AWS_S3_DISABLE_SSL=true
export AWS_REGION=us-east-1

# MinIO-style environment variables (for mc CLI fallback)
# NOTE: Commented out to force built-in SDK usage for testing
# export MINIO_ACCESS_KEY=myaccesskey
# export MINIO_SECRET_KEY=mysecretkey

echo "MinIO test environment loaded"
echo "AWS_ENDPOINT_URL: $AWS_ENDPOINT_URL"
echo "AWS_ACCESS_KEY_ID: $AWS_ACCESS_KEY_ID"
